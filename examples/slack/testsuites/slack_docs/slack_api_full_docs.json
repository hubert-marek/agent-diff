    "conversations.list": {
        "description": "Lists all channels in a Slack team.",
        "parameters": {
            "body": {
                "token": {
                    "type": "string",
                    "required": true,
                    "description": "Authentication token bearing required scopes."
                },
                "cursor": {
                    "type": "string",
                    "required": false,
                    "description": "Paginate through collections of data by setting cursor to a next_cursor value."
                },
                "exclude_archived": {
                    "type": "boolean",
                    "required": false,
                    "description": "Set to true to exclude archived channels. Default: false."
                },
                "limit": {
                    "type": "integer",
                    "required": false,
